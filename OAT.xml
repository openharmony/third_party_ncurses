                <filteritem type="filepath" name="test/.*" desc="InvalidLicense Filter"/>
            </filefilter>
            <filefilter name="copyrightPolicyFilter" desc="Filters for copyright header policies">
                <filteritem type="filename" name="ncurses.spec"/>
                <filteritem type="filename" name="ncurses.yaml"/>
                <filteritem type="filename" name="ANNOUNCE|configure|dist.mk|install-sh|modules|TODO|TO-DO|MANIFEST|VERSION" desc="InvalidCopyright file"/>
                <filteritem type="filename" name="*.m4|*.sh|*.awk|*.ads|*.adb|*.adb_p|*.1|*.1m|*.3x|*.5|*.cc|*.cmd|*.def|*.hin|*.sed|*.spec|*.tail|README*|config.*|*ncurses_defs|Makefile.*" desc="InvalidCopyright file"/>
                <filteritem type="filepath" name="Ada95/gen/gen.c" desc="InvalidCopyright file"/>
                <filteritem type="filepath" name="Ada95/package/debian/.*" desc="InvalidLicense Filter"/>
                <filteritem type="filepath" name="Ada95/src/c_*.*" desc="InvalidLicense Filter"/>
                <filteritem type="filepath" name="Ada95/src/ncurses_compat.c" desc="InvalidLicense Filter"/>
                <filteritem type="filename" name="PROBLEMS|curses*.*|cursslk.h|headers|internal.h" desc="InvalidCopyright Filter"/>
