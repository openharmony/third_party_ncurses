                <filteritem type="filepath" name="man/term*.*" desc="InvalidCopyright Filter"/>
                <filteritem type="filepath" name="menu/.*" desc="InvalidCopyright Filter"/>
                <filteritem type="filepath" name="misc/.*" desc="InvalidCopyright Filter"/>
                <filteritem type="filepath" name="ncurses/.*.c" desc="InvalidCopyright Filter"/>
                <filteritem type="filepath" name="ncurses/.*.h" desc="InvalidCopyright Filter"/>
                <filteritem type="filepath" name="ncurses/llib.*" desc="InvalidCopyright Filter"/>
                <filteritem type="filepath" name="package/.*" desc="InvalidCopyright Filter"/>
                <filteritem type="filepath" name="panel/.*" desc="InvalidCopyright Filter"/>
                <filteritem type="filepath" name="progs/.*" desc="InvalidCopyright Filter"/>
                <filteritem type="filepath" name="test/.*" desc="InvalidCopyright Filter"/>
            </filefilter>
        </filefilterlist>
    </oatconfig>
